	<field name="T_PEDIDOS_FEC_SOLICITADA" class="java.sql.Timestamp"/>
	<field name="T_CLIENTES_RAZON_SOCIAL" class="java.lang.String"/>
	<field name="T_PRODUCTOS_NOMBRE" class="java.lang.String"/>
