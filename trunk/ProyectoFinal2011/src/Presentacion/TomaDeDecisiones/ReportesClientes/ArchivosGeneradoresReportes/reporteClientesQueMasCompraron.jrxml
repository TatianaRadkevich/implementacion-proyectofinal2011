	<parameter name="ID_PROVINCIA" class="java.lang.Integer"/>
	<parameter name="FECHA_DESDE" class="java.util.Date"/>
	<parameter name="FECHA_HASTA" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT TOP 10
     SUM(T_DETALLES_FACTURA."PRECIO"*T_DETALLES_FACTURA."CANTIDAD") AS 'MONTO_TOTAL_COBRADO',
     T_PROVINCIAS."NOMBRE" AS T_PROVINCIAS_NOMBRE,
     T_CLIENTES."ID_CLIENTE" AS T_CLIENTES_ID_CLIENTE,
     T_CLIENTES."RAZON_SOCIAL" AS T_CLIENTES_RAZON_SOCIAL,
     T_TCLIENTE."NOMBRE" AS T_TCLIENTE_NOMBRE
FROM
     "dbo"."T_DOMICILIOS" T_DOMICILIOS INNER JOIN "dbo"."T_CLIENTES" T_CLIENTES ON T_DOMICILIOS."ID_DOMICILIO" = T_CLIENTES."ID_DOMICILIO"
     INNER JOIN "dbo"."T_PEDIDOS" T_PEDIDOS ON T_CLIENTES."ID_CLIENTE" = T_PEDIDOS."ID_CLIENTE"
     INNER JOIN "dbo"."T_TCLIENTE" T_TCLIENTE ON T_CLIENTES."ID_TCLIENTE" = T_TCLIENTE."ID_TCLIENTE"
     INNER JOIN "dbo"."T_FACTURAS" T_FACTURAS ON T_PEDIDOS."ID_PEDIDO" = T_FACTURAS."ID_PEDIDO"
     INNER JOIN "dbo"."T_DETALLES_FACTURA" T_DETALLES_FACTURA ON T_FACTURAS."ID_FACTURA" = T_DETALLES_FACTURA."ID_FACTURA"
     INNER JOIN "dbo"."T_EFACTURA" T_EFACTURA ON T_FACTURAS."ID_EFACTURA" = T_EFACTURA."ID_EFACTURA"
     INNER JOIN "dbo"."T_PROVINCIAS" T_PROVINCIAS ON T_DOMICILIOS."ID_PROVINCIA" = T_PROVINCIAS."ID_PROVINCIA"
WHERE
     T_PROVINCIAS."ID_PROVINCIA" = $P{ID_PROVINCIA}
 AND T_FACTURAS."FEC_FACTURA" BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
 AND T_FACTURAS."ID_EFACTURA" = ( SELECT "T_EFACTURA"."ID_EFACTURA" FROM "T_EFACTURA" WHERE T_EFACTURA."NOMBRE" LIKE 'Cobrada' )
GROUP BY
     T_CLIENTES."ID_CLIENTE",
     T_CLIENTES."RAZON_SOCIAL",
     T_TCLIENTE."NOMBRE",
     T_PROVINCIAS."NOMBRE"
ORDER BY
     MONTO_TOTAL_COBRADO DESC]]>
	</queryString>
