	<parameter name="id_recibo" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     R."FEC_HORA_COBRO" AS FEC_HORA_COBRO,
     R."IMPORTE" AS IMPORTE,
     R."OBSERVACIONES" AS OBSERVACIONES,
     FP."NOMBRE" AS FORMAS_PAGO_NOMBRE,
     F."NUMERO" AS NUMERO,
     C."RAZON_SOCIAL" AS RAZON_SOCIAL,
     C."CUIT" AS CUIT
FROM  "dbo"."T_COBROS" R
	LEFT OUTER JOIN "dbo"."T_FORMAS_PAGO" FP
		ON FP."ID_FORMA_PAGO" = R."ID_FORMA_PAGO"
	INNER JOIN "dbo"."T_FACTURAS" F
		INNER JOIN "dbo"."T_PEDIDOS" P
			INNER JOIN "dbo"."T_CLIENTES" C
				ON P."ID_CLIENTE" = C."ID_CLIENTE"
				ON F."ID_PEDIDO" = P."ID_PEDIDO"
				ON R."ID_FACTURA" = F."ID_FACTURA"
WHERE R.ID_COBRO = $P{id_recibo}]]>
	</queryString>
