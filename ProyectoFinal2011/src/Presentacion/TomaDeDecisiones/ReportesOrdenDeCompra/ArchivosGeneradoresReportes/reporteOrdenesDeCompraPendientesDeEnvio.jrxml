	</parameter>
	<parameter name="FECHA_DESDE" class="java.util.Date"/>
	<parameter name="FECHA_HASTA" class="java.util.Date"/>
	<parameter name="WHERE_ID_PROVEEDOR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[(($P{ID_PROVEEDOR} != null) ? " AND T_ORDENES_COMPRA.ID_PROVEEDOR = " + $P{ID_PROVEEDOR} : "")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     T_ORDENES_COMPRA."ID_ORDEN_COMPRA" AS T_ORDENES_COMPRA_ID_ORDEN_COMPRA,
     T_ORDENES_COMPRA."FEC_GENERACION" AS T_ORDENES_COMPRA_FEC_GENERACION,
     T_PROVEEDORES."RAZON_SOCIAL" AS T_PROVEEDORES_RAZON_SOCIAL
FROM
     "dbo"."T_PROVEEDORES" T_PROVEEDORES INNER JOIN "dbo"."T_ORDENES_COMPRA" T_ORDENES_COMPRA ON T_PROVEEDORES."ID_PROVEEDOR" = T_ORDENES_COMPRA."ID_PROVEEDOR"
     INNER JOIN "dbo"."T_EORDEN_COMPRA" T_EORDEN_COMPRA ON T_ORDENES_COMPRA."ID_EORDEN_COMPRA" = T_EORDEN_COMPRA."ID_EORDEN_COMPRA"
WHERE
     T_ORDENES_COMPRA."ID_EORDEN_COMPRA" = ( SELECT "T_EORDEN_COMPRA"."ID_EORDEN_COMPRA" FROM "T_EORDEN_COMPRA" WHERE T_EORDEN_COMPRA."NOMBRE" LIKE 'Pendiente' )
 AND T_ORDENES_COMPRA."FEC_GENERACION" BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
$P!{WHERE_ID_PROVEEDOR}]]>
	</queryString>
