	<queryString>
		<![CDATA[SELECT
     E."ID_EMPLEADO" AS ID_EMPLEADO,
     E."NOMBRE" AS NOMBRE,
     E."APELLIDO" AS APELLIDO,
     E."NUMERO_DOCUMENTO" AS NUMERO_DOCUMENTO,
     OT."ID_ORDEN_TRABAJO" AS ID_ORDEN_TRABAJO,
     OT."FEC_EMISION" AS FEC_EMISION,
     DP."FEC_HORA_PREVISTA_INICIO" AS FEC_HORA_PREVISTA_INICIO,
     DP."FEC_HORA_PREVISTA_FIN" AS FEC_HORA_PREVISTA_FIN,
     DP."CANTIDAD_PLANIFICADA" AS CANTIDAD,
     DP."OBSERVACIONES" AS OBSERVACIONES
FROM "dbo"."T_ORDENES_TRABAJO" OT
     INNER JOIN "dbo"."T_EMPLEADOS" E
         ON E."ID_EMPLEADO" = OT."ID_EMPLEADO"
     INNER JOIN "dbo"."T_DETALLES_PLAN" DP
         ON OT."ID_ORDEN_TRABAJO" = DP."ID_ORDEN_TRABAJO"
WHERE
     OT.ID_ORDEN_TRABAJO = $P{id_orden_trabajo}]]>
	</queryString>
